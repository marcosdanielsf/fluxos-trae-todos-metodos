# Copie este arquivo para .env e preencha com suas credenciais

# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key-here

# Pinecone
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=trae-os-memory

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/trae_os
REDIS_URL=redis://localhost:6379/0

# API
API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=your-secret-key-here

# CRM Socialfy
SOCIALFY_API_KEY=your-socialfy-api-key
SOCIALFY_BASE_URL=https://api.socialfy.com.br

# Email (SendGrid)
SENDGRID_API_KEY=your-sendgrid-api-key
FROM_EMAIL=noreply@seudominio.com

# Google Calendar
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8000/oauth/callback

# Twilio (WhatsApp)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_WHATSAPP_NUMBER=+14155238886

# Monitoring
PROMETHEUS_PORT=9090
LOG_LEVEL=INFO

# Environment
ENVIRONMENT=development
DEBUG=true
